{
  "name": "nxt-watch",
  "private": true,
  "version": "1.0.0",
  "engines": {
    "node": ">=14",
    "npm": ">=6"
  },
  "dependencies": {
    "@craco/craco": "7.1.0",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@testing-library/user-event": "14.4.3",
    "chalk": "4.1.2",
    "date-fns": "2.30.0",
    "history": "5.3.0",
    "jest-styled-components": "7.1.1",
    "js-cookie": "3.0.5",
    "msw": "1.3.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "4.12.0",
    "react-loader-spinner": "4.0.0",
    "react-player": "2.14.1",
    "react-router-dom": "5.3.4",
    "react-slick": "0.30.3",
    "react-speech-kit": "3.0.1",
    "react-toastify": "9.1.3",
    "reactjs-popup": "2.0.5",
    "slick-carousel": "1.8.1",
    "styled-components": "6.1.8",
    "surge": "0.23.1"
  },
  "devDependencies": {
    "@babel/eslint-parser": "7.28.4",
    "@babel/preset-react": "7.27.1",
    "ajv": "6.12.6",
    "ajv-keywords": "3.5.2",
    "babel-eslint": "10.1.0",
    "babel-plugin-styled-components": "2.1.1",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.2.1",
    "husky": "8.0.3",
    "lint-staged": "15.2.7",
    "npm-run-all": "4.1.5",
    "prettier": "3.3.3",
    "react-scripts": "5.0.1"
  },
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "craco test",
    "lint": "eslint .",
    "lint:fix": "eslint --fix src/",
    "format": "prettier --write \"./src\"",
    "run-all": "npm-run-all --parallel test lint:fix"
  },
  "lint-staged": {
    "*.js": [
      "npm run lint:fix"
    ],
    "*.{js, jsx, json, html, css}": [
      "npm run format"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js"
    ]
  },
  "resolutions": {
    "ajv": "6.12.6",
    "ajv-keywords": "3.5.2",
    "ajv-formats": "1.6.1"
  },
  "browserslist": {
    "development": [
      "last 2 chrome versions",
      "last 2 firefox versions",
      "last 2 edge versions"
    ],
    "production": [
      ">1%",
      "last 4 versions",
      "Firefox ESR",
      "not ie < 11"
    ]
  }
}